<?xml version="1.0" encodign="utf-8"?>
<odoo>
    <record id="group_cooperative_admin" model="res.groups">
        <field name="name">Cooperative / Admin</field>
    </record>
    
    <record id="only_active_modify" model="ir.rule">
        <field name="name">Only Active Task can be modified</field>
        <field name="perm_read" eval="0"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="0"/>
        <field name="perm_unlink" eval="1"/>
        <field name="domain_force">[('active','=',True)]</field>
        <field name="model_id" ref="model_cooperative_task"/>
    </record>
</odoo>